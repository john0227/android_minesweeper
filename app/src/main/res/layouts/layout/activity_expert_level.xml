<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:text="Expert"
        android:textColor="#A6A3A3"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include layout="@layout/image_button_setting" />

    <RadioGroup
        android:id="@+id/rg_sel_flag"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="20dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <RadioButton
            android:id="@+id/rbtn_sel"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:background="@drawable/radiobutton_toggle_click"
            android:button="@null"
            android:checked="true" />

        <RadioButton
            android:id="@+id/rbtn_flag"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:background="@drawable/radiobutton_toggle_flag"
            android:button="@null" />
    </RadioGroup>

    <ImageView
        android:id="@+id/iv_mine_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:contentDescription="Mine Image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mine" />

    <TextView
        android:id="@+id/tv_mine_count"
        android:layout_width="100dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:background="#A6A3A3"
        android:gravity="center"
        android:text="Mine Count"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_mine_count"
        app:layout_constraintStart_toEndOf="@+id/iv_mine_count"
        app:layout_constraintTop_toTopOf="@+id/iv_mine_count" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="100dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:background="#A6A3A3"
        android:gravity="center"
        android:text="0"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_mine_count"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_mine_count" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/rg_sel_flag"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_mine_count">

        <LinearLayout
            android:id="@+id/linearLayout0"
            android:layout_width="350dp"
            android:layout_height="25dp"
            android:background="#595757"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/iv_mine_count">
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout1"
